{
    "permissions": [
        "bookmarks",
        "contextMenus",
        "activeTab", 
        "tabs"
    ],
  "name": "BookMark ver.1",
  "description": "BookMark Testing ver.1",
  "version": "1.1",
  "manifest_version": 2,
  "background": {
        "scripts": ["tf.min.js"],
        "persistent": false
  },
  "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}